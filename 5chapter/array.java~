import java.util.*;
public class array{
	public static void main(String[] args){
		int[] mylist = new int[] {1,3,5,7,9,10};
		int[] mylist3 = new int[] {1,3,5,7,9};
		int[] mylist2 = new int[5];
		System.out.println(Arrays.mismatch(mylist,mylist3));
		System.out.println(Arrays.compare(mylist,mylist3));
		System.out.println(Arrays.binarySearch(mylist3,4));
		mylist2[0]=1;
		Arrays.sort(mylist);
		System.out.println(Arrays.binarySearch(mylist,8));
		
		for(int i: mylist){
			System.out.println(i);
		}
		for(int i: mylist2){
			System.out.println(i);
		}
	}
}
