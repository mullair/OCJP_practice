public class FinalBlock{
	public static void main(String[] args){
		StringBuilder sb = new StringBuilder();
		try{
			throw new Exception();
		//	sb.append("a");
		}
		catch(Exception e){
			sb.append("c");
			System.exit(0);
		}
		finally{
			System.out.println("hey du");
			sb.append("b");
		}
		System.out.println(sb);
	}
}
